<!-- This is an example file of how a testcontent.xml could look.
     Use your own file for your test content. Please note that tags are case sensitive.
     This file should be placed under your Environment.getExternalStorageDirectory() -->

<TestContent> <!-- Tag holding one test content -->
    <Id>LOCAL</Id> <!-- Used to identify content when testing -->
    <ContentURI>/sdcard/video_file.mp4</ContentURI> <!-- Path / URL to content -->
    <Width>1920</Width> <!-- Width of video, pixels -->
    <Height>1080</Height> <!-- Height of video, pixels -->
    <Duration>000520</Duration> <!-- Duration in format HHMMSS -->
    <Framerate>29.9997</Framerate> <!-- Framerate for the content -->
    <BitRate>8968</BitRate> <!-- Bitrate for the content -->
    <maxIFrameInterval>2000</maxIFrameInterval> <!-- Maximum interval between I-frames in media -->
    <TrackCount>2</TrackCount> <!-- Number of tracks in the container -->
    <MimeType>video/avi</MimeType> <!-- MimeType for the container -->
    <TrackMimeTypeVideo>video/avc</TrackMimeTypeVideo> <!-- MimeType for the VideoTrack -->
    <TrackMimeTypeAudio>audio/mp4a-latm</TrackMimeTypeAudio> <!-- MimeType for the AudioTrack -->
    <Title>TestTitle</Title> <!-- MetaData Title -->
    <Album>TestAlbum</Album> <!-- MetaData Album -->
    <Artist>TestArtist</Artist> <!-- MetaData Artist -->
    <AlbumArtist>TestAlbumArtist</AlbumArtist> <!-- MetaData AlbumArtist -->
    <Genre>TestGenre</Genre> <!-- MetaData Genre -->
    <TrackNumber>1/1</TrackNumber> <!-- MetaData TrackNumber -->
    <Compilation>0</Compilation> <!-- MetaData Compilation -->
    <Author>TestLyricist</Author> <!-- MetaData Author -->
    <Writer>TestComposer</Writer> <!-- MetaData Writer -->
    <DiscNumber>1</DiscNumber> <!-- MetaData Disc number -->
    <Year>2014</Year> <!-- MetaData Year -->
</TestContent>
